{
"manifest_version": 2,
    "name": "WordBlocker Pre-Alpha",
    "description": "WordBlocker, The descreet internet blindfold.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}